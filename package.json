{
	"name": "client",
	"version": "0.1.0",
	"private": true,
	"homepage": "http://MohammedFaragallah.github.io",
	"scripts": {
		"analyze": "yarn build:dev && source-map-explorer 'build/static/js/*.js'",
		"build": "cross-env GENERATE_SOURCEMAP=false yarn run build:dev",
		"build:dev": "react-app-rewired build",
		"dev": "react-app-rewired start",
		"eject": "react-script eject",
		"lint": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
		"serve": "serve -s build",
		"sort": "sort-package-json package.json",
		"start": "react-app-rewired start",
		"sync": "syncyarnlock -s -k -g -l",
		"test": "react-app-rewired test",
		"predeploy": "yarn build",
		"deploy": "gh-pages -d build",
		"update": "yarn upgrade-interactive"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@loadable/component": "^5.12.0",
		"@material-ui/core": "^4.9.0",
		"@sentry/browser": "^5.11.1",
		"autotrack": "^2.4.1",
		"clsx": "^1.0.4",
		"color": "^3.1.2",
		"connected-react-router": "^6.6.1",
		"flat": "^5.0.0",
		"formik": "^2.1.2",
		"history": "^4.10.1",
		"jss": "^10.0.3",
		"jss-rtl": "^0.3.0",
		"pluralize": "^8.0.0",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-anchor-link-smooth-scroll": "^1.0.12",
		"react-app-rewired": "^2.1.5",
		"react-color": "^2.18.0",
		"react-dom": "^16.12.0",
		"react-ga": "^2.7.0",
		"react-helmet-async": "^1.0.4",
		"react-intl": "^3.11.0",
		"react-keyboard-event-handler": "^1.5.3",
		"react-parallax": "^3.0.3",
		"react-redux": "^7.1.3",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.3.0",
		"react-scrollspy": "^3.4.2",
		"react-waypoint": "^9.0.2",
		"redux": "^4.0.5",
		"redux-thunk": "^2.3.0",
		"slugify": "^1.3.6"
	},
	"devDependencies": {
		"babel-plugin-optimize-clsx": "^2.5.0",
		"cross-env": "^6.0.3",
		"customize-cra": "^0.9.1",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-universe": "^2.1.0",
		"eslint-plugin-import": "^2.20.0",
		"gh-pages": "^2.2.0",
		"prettier": "^1.19.1",
		"source-map-explorer": "^2.2.2",
		"syncyarnlock": "^1.0.19",
		"typescript": "^3.7.5"
	},
	"engines": {
		"yarn": ">= 1.19.1"
	}
}

		"@testing-library/user-event": "^12.6.0",
		"@types/color": "^3.0.1",
		"autotrack": "^2.4.1",
		"axios": "^0.21.2",
		"axios-retry": "^3.1.9",
		"clsx": "^1.1.1",
		"color": "^3.1.3",
		"connected-react-router": "^6.8.0",
		"flat": "^5.0.2",

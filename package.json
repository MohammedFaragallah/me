{
	"name": "client",
	"version": "0.1.0",
	"private": true,
	"homepage": "http://MohammedFaragallah.github.io",
	"scripts": {
		"analyze": "yarn build:dev && source-map-explorer 'build/static/js/*.js'",
		"build": "cross-env GENERATE_SOURCEMAP=false yarn run build:dev",
		"build:dev": "react-scripts build",
		"predeploy": "yarn build",
		"deploy": "gh-pages -d build",
		"dev": "react-scripts start",
		"eject": "react-script eject",
		"lint": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
		"push": "yarn && git add . && git commit -m 'save' && git push",
		"push:heroku": "yarn push heroku master",
		"serve": "serve -s build",
		"sort": "sort-package-json package.json",
		"start": "react-scripts start",
		"sync": "syncyarnlock -s -k -g -l",
		"test": "react-scripts test",
		"update": "yarn upgrade-interactive"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@feathersjs/authentication-client": "^4.5.1",
		"@feathersjs/feathers": "^4.5.1",
		"@feathersjs/rest-client": "^4.5.1",
		"@loadable/component": "^5.12.0",
		"@material-ui/core": "^4.9.0",
		"@sentry/browser": "^5.11.1",
		"@stickyroll/stickyroll": "^1.2.0",
		"@testing-library/jest-dom": "^5.1.1",
		"@testing-library/react": "^9.4.0",
		"@testing-library/user-event": "^8.1.1",
		"@types/color": "^3.0.1",
		"autotrack": "^2.4.1",
		"axios": "^0.19.2",
		"axios-retry": "^3.1.2",
		"clsx": "^1.0.4",
		"color": "^3.1.2",
		"connected-react-router": "^6.6.1",
		"flat": "^5.0.0",
		"formik": "^2.1.2",
		"get-user-locale": "^1.3.0",
		"history": "^4.10.1",
		"jss": "^10.0.3",
